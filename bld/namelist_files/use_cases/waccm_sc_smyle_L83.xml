<tracer_cnst_type     >   'CYCLICAL'                                                                                    </tracer_cnst_type>
<tracer_cnst_filelist >   ''                                                                                            </tracer_cnst_filelist>

<!-- emissions -->

<!-- External forcing -->
<ext_frc_type>CYCLICAL</ext_frc_type>
<ext_frc_cycle_yr>1850</ext_frc_cycle_yr>

<!-- Surface emissions -->
<srf_emis_type>CYCLICAL</srf_emis_type>
