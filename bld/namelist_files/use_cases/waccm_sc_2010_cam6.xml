<prescribed_ozone_datapath>   'atm/cam/ozone'                                           </prescribed_ozone_datapath>
<prescribed_ozone_file    >   'ozone_strataero_CAM6chem_2010climo_zm_5day_c171004.nc'   </prescribed_ozone_file>
<prescribed_ozone_name    >   'O3'                                                      </prescribed_ozone_name>
<prescribed_ozone_type    >   'CYCLICAL'                                                </prescribed_ozone_type>

<!-- Prescribed stratospheric aerosols -->
<prescribed_strataero_cycle_yr    >   2010                                                      </prescribed_strataero_cycle_yr>
<prescribed_strataero_datapath    >   'atm/cam/ozone'                                           </prescribed_strataero_datapath>
