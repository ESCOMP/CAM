<?xml version="1.0"?>

<namelist_defaults>

<!-- Solar data -->
<solar_irrad_data_file>atm/cam/solar/SolarForcing1995-2005avg_c160929.nc</solar_irrad_data_file>
<solar_data_ymd>20100101</solar_data_ymd>
<solar_data_type>FIXED</solar_data_type>

<!-- Kp and F10.7 -->
